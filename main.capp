int update() {
    while (position > 0.0) {
        // Update velocity: v = u + at
        velocity = velocity + gravity * dt;
        
        // Update position: s = s - vt
        position = position - velocity * dt;

        if (position < 0.0) {
            position = 0.0;
        }
    }
}

